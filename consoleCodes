var text = prompt("Enter text");
var text140 = text.slice(0,140);
alert(text140);

alert(prompt("Enter text: ").slice(0,140));  //3 lines into 1 line

var name = "Faheela";
name.toUpperCase();
name.toLowerCase();

var name = prompt("Enter your name");
var char1 = name.slice(0,1); //Take 1st letter
var char1Upper = char1.toUpperCase(); //Capitalize first letter
var restName = name.slice(1,name.length); //Take rest of name
var restNameLower = restName.toLowerCase(); //Make rest of name lower case
var capName = char1Upper + restNameLower; //Concatenate capitalized first letter and rest of name.
alert("Hello " + capName);

var dogAge = prompt("Enter dog age:");
var humanAge = (dogAge - 2) * 4 + 21;
alert(humanAge);

